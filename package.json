    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.1",
    "@biomejs/biome": "1.9.4",
    "@markuplint/html-parser": "^4.6.19",
    "@markuplint/html-spec": "^4.14.2",
    "@markuplint/jsx-parser": "^4.7.19",
