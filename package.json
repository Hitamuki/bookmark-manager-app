{
  "name": "bookmark-manager-app",
  "private": true,
  "version": "1.0.0",
  "author": "Hitamuki",
  "description": "",
  "scripts": {
    "dev": "nx serve",
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "format:check": "nx run-many -t format:check",
    "format": "nx run-many -t format",
    "lint": "nx run-many -t lint",
    "lint:md": "markdownlint-cli2 '**/*.md'",
    "lint:txt": "textlint .",
    "lint:fix": "nx run-many -t lint:fix",
    "lint:md:fix": "markdownlint-cli2 '**/*.md' --fix",
    "check:spell": "cspell .",
    "check:links": "markdown-link-check ./docs/spec",
    "check:axe": "axe http://localhost:3000",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph",
    "list": "nx list",
    "list:pj": "nx show projects",
    "prepare": "husky && npx browser-driver-manager install chrome"
  },
  "lint-staged": {
    "*.md": ["markdownlint-cli2", "markdown-link-check", "textlint"],
    "*.{ts,tsx,js,jsx,mjs,cjs}": ["biome format --write", "biome lint", "markuplint"],
    "*.html": ["prettier --write", "markuplint"],
    "*.css": ["biome format --write", "stylelint", "biome lint"],
    "*.scss": ["prettier --write", "stylelint"],
    "*.{json,jsonc}": ["biome format --write"],
    "*.{yml,yaml,toml}": ["prettier --write"]
  },
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10.8.1",
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!"
  },
